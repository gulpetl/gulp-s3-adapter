{
  "name": "@gulpred/s3-adapter",
  "version": "0.0.3",
  "repository": "https://github.com/gulpetl/gulp-s3-adapter",
  "description": "Upload/download files to/from S3",
  "main": "dist/js/plugin.js",
  "typings": "dist/types/plugin.d.ts",
  "files": [
    "dist",
    "s3_src.*",
    "s3_dest.*"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist/* --glob",
    "clean": "rimraf dist/* --glob",
    "test": "vitest",
    "bump": "npm version patch && git push --tags origin master"
  },
  "author": "peterl@sbcotton.com",
  "license": "MIT",
  "node-red": {
    "version": ">=2.0.0",
    "nodes": {
      "s3.src": "s3_src.js",
      "s3.dest": "s3_dest.js"
    }
  },
  "keywords": [
    "s3",
    "gulpfriendly",
    "vinyl",
    "gulp",
    "gulpfriendly",
    "gulp-etl",
    "gulpetl",
    "etl",
    "node-red"
  ],
  "homepage": "https://gulpetl.com",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.692.0",
    "from2": "^2.3.0",
    "gulp-error-handle": "^1.0.1",
    "loglevel": "^1.9.1",
    "merge": "^2.1.1",
    "minio": "^8.0.2",
    "pkginfo": "^0.4.1",
    "plugin-error": "^2.0.1",
    "rimraf": "^6.0.1",
    "stream-combiner": "^0.2.2",
    "through2": "^4.0.2",
    "vinyl": "^3.0.0"
  },
  "devDependencies": {
    "@types/from2": "^2.3.5",
    "@types/node": "^18.19.50",
    "@types/request": "^2.48.12",
    "@types/vinyl": "^2.0.12",
    "gulp": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vitest": "^2.0.5"
  }
}
